{
  "expo": {
    "name": "Jahatelo",
    "slug": "jahatelo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-1024.png",
    "scheme": "jahatelo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#822DE2",
    "splash": {
      "image": "./assets/splash-2732.png",
      "resizeMode": "contain",
      "backgroundColor": "#822DE2"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/ios-icon-1024.png",
      "bundleIdentifier": "app.jahatelo.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci√≥n para mostrarte moteles cercanos."
      }
    },
    "android": {
      "package": "app.jahatelo.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-foreground-1080.png",
        "backgroundColor": "#822DE2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#822DE2",
          "dark": {
            "backgroundColor": "#822DE2"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
