{
  "expo": {
    "name": "Jahatelo",
    "slug": "jahatelo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon-1024.png",
    "scheme": "jahatelo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#822DE2",
    "splash": {
      "image": "./assets/splash-icon-v2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/ios-icon-1024.png",
      "bundleIdentifier": "app.jahatelo.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci√≥n para mostrarte moteles cercanos."
      }
    },
    "android": {
      "package": "app.jahatelo.mobile",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#822DE2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-v2.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#FFFFFF"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "98e7ed1c-2b9c-45e9-8058-4e8f94cd7235"
      },
      "router": {},
      "googleClientIdExpo": "538770919156-6c7dh7o9ddifj0hd8e75rlbtlk9jem08.apps.googleusercontent.com",
      "googleClientIdIos": "538770919156-iguu9u41nf7fakboh06vvm09be25ip38.apps.googleusercontent.com",
      "googleClientIdAndroid": "538770919156-f9enp1tnesjus884vk8tvi047utr3gfh.apps.googleusercontent.com",
      "googleClientIdWeb": "538770919156-pioslijk4die35brv5njd5pk4k5fcfbe.apps.googleusercontent.com"
    },
    "owner": "jmongelos"
  }
}
