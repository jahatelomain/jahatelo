# Variables de entorno para producción
# Expo lee variables que comienzan con EXPO_PUBLIC_

# URL del API backend (Next.js)
# Cambiar esta URL cuando se despliegue a producción
EXPO_PUBLIC_API_URL=https://jahatelo.vercel.app

# Caché habilitado en producción (0 = activado, 1 = desactivado)
# El caché mejora el rendimiento y reduce el consumo de datos
EXPO_PUBLIC_DISABLE_CACHE=0
